{
  "name": "@swim-io/ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development SKIP_PREFLIGHT_CHECK=true DISABLE_ESLINT_PLUGIN=true craco start",
    "build": "craco build",
    "release": "SENTRY_RELEASE=$(git rev-parse --short HEAD) SKIP_PREFLIGHT_CHECK=true DISABLE_ESLINT_PLUGIN=true craco build",
    "test": "NODE_ENV=development SKIP_PREFLIGHT_CHECK=true craco test --transformIgnorePatterns \"node_modules/(?!@project-serum/sol-wallet-adapter)/\"",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "types:generate": "typechain --target=ethers-v5 --out-dir=src/contracts/ abi/*.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@certusone/wormhole-sdk": "^0.2.6",
    "@craco/craco": "^6.3.0",
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^38.2.0",
    "@ethers-ancillary/bsc": "^0.0.3",
    "@ledgerhq/hw-transport-webusb": "^6.0.2",
    "@project-serum/borsh": "^0.2.2",
    "@project-serum/sol-wallet-adapter": "^0.2.4",
    "@sayari/eslint-plugin": "^0.0.1-rc.4",
    "@sentry/react": "^6.19.2",
    "@sentry/tracing": "^6.19.2",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.33.0",
    "bn.js": "^5.2.0",
    "constate": "^3.3.0",
    "decimal.js": "^10.3.1",
    "ethers": "^5.4.4",
    "jscrypto": "^1.0.3",
    "moment": "^2.29.1",
    "moralis": "^1.3.5",
    "node-sass": "^4.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "3.25.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.5",
    "wasm-loader": "^1.3.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.18.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "^2.3.2",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^28.0.2",
    "typechain": "^5.1.2",
    "typescript": "~4.2",
    "webpack": "4.44.2"
  },
  "browser": {
    "crypto": false
  }
}
