    "eslint-plugin-react": "^7.29.4",
    "jest-mock-extended": "^2.0.6",
    "prettier": "^2.7.1",
    "typechain": "^5.2.0",
    "typescript": "~4.7.4",
    "webpack": "4.44.2"
  }
